package com.electronicsworld.model;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;



@Entity
public class Product 
{
	@Id
	@GeneratedValue
	int pid;
	String pname;
	String pspecification;
	float pcost;
	
	public Product() {
		
	}

	public int getPid() {
		return pid;
	}

	public void setPid(int pid) {
		this.pid = pid;
	}

	public String getPname() {
		return pname;
	}

	public void setPname(String pname) {
		this.pname = pname;
	}

	public String getPspecification() {
		return pspecification;
	}

	public void setPspecification(String pspecification) {
		this.pspecification = pspecification;
	}

	public float getPcost() {
		return pcost;
	}

	public void setPcost(float pcost) {
		this.pcost = pcost;
	}
	
	
	
	
}
